{
  "name": "api-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "node ./scripts/start.js",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development node ./scripts/start.js",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node ./scripts/start.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api-server/test/jest-e2e.json",
    "start:api-server": "cross-env NODE_ENV=development nest start api-server --watch",
    "start:user-manage-server": "nest start user-manage-server --watch",
    "start:shopping-server": "nest start shopping-server --watch",
    "start:live-server": "nest start live-server --watch",
    "start:upload-server": "nest start upload-server --watch",
    "start:chat-gpt-server": "nest start chat-gpt-server --watch"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@liaoliaots/nestjs-redis": "^8.2.2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/microservices": "^10.2.10",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^9.0.1",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "bull": "^4.11.3",
    "chalk": "4.1.2",
    "child_process": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "concurrently": "^8.2.2",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "3.0.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "log4js": "^6.8.0",
    "moment": "^2.29.4",
    "mysql2": "^3.3.3",
    "node-media-server": "^2.6.2",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.6.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/decorators(|/.*)$": "<rootDir>/libs/decorators/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/types(|/.*)$": "<rootDir>/libs/types/src/$1",
      "^@app/auth(|/.*)$": "<rootDir>/libs/auth/src/$1"
    }
  }
}